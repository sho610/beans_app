<%= form_with model:shop,local:true do |f| %>
  <div class="form-group">
    <%= f.label :"ショップ" %>
    <%= f.text_field :name, class: 'form-control book_title' %>
  </div>
  <div class="form-group">
    <%= f.label :"概要" %>
    <%= f.text_area :overview, class: 'form-control book_body' %>
  </div>
  <div class = "form_group" >
    <%= f.fields_for :prefecture do |pre| %>
      <%= pre.hidden_field :user_id, value: current_user.id %>
      <%= pre.label :地域選択 %><br>
      <%= pre.select :name, Prefecture.names.keys, {prompt: '選択してください'}, class: 'form-control btn btn-info' %>
    <% end %>
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
  <!--<#%= render "prefectures/form" , prefecture: @prefecture%>-->
<% end %>