<%= form_with model:shop,local:true do |f| %>
  <div class="form-group">
    <%= f.label :"ショップ" %>
    <%= f.text_field :name, class: 'form-control book_title' %>
  </div>
  <div class="form-group">
    <%= f.label :"概要" %>
    <%= f.text_area :overview, class: 'form-control book_body' %>
  </div>
  <div class="form-group">
    <%= f.submit class: 'btn btn-success' %>
  </div>
<% end %>